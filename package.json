{
  "name": "undefinedschool.github.io",
  "version": "1.0.0",
  "description": "[![Netlify Status](https://api.netlify.com/api/v1/badges/463bac1e-49c9-49a3-897b-84239a115496/deploy-status)](https://app.netlify.com/sites/undefinedschool/deploys)",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.1",
    "cssnano": "^4.1.10",
    "husky": "^3.0.9",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.3"
  },
  "scripts": {
    "minify:html": "html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-attribute-quotes --remove-tag-whitespace --use-short-doctype src/index.html -o dist/index.html",
    "build:css": "postcss src/styles.css -o dist/styles.css",
    "watch:css": "postcss src/styles.css -o dist/styles.css -w",
    "minify:js": "terser -c -m -o dist/scripts.js -- src/scripts.js",
    "build": "npm run minify:html && npm run build:css && npm run minify:js",
    "dev": "live-server dist"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undefinedschool/undefinedschool.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/undefinedschool/undefinedschool.github.io/issues"
  },
  "homepage": "https://github.com/undefinedschool/undefinedschool.github.io#readme"
}
